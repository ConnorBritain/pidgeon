{
  "segment": "PV2",
  "name": "Patient Visit - Additional Information",
  "description": "Contains extended patient visit information and administrative details",
  "standard": "hl7v23",
  "version": "2.3",
  "usage": "O",
  "cardinality": {
    "min": 0,
    "max": 1
  },
  "fields": {
    "PV2.1": {
      "name": "Prior Pending Location",
      "description": "Previous pending location for patient",
      "dataType": "PL",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["ICU^201^A^MAIN^P"]
    },
    "PV2.2": {
      "name": "Accommodation Code",
      "description": "Type of accommodation requested or assigned",
      "dataType": "CE",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["PR^Private room", "SP^Semi-private"]
    },
    "PV2.3": {
      "name": "Admit Reason",
      "description": "Reason for admission",
      "dataType": "CE",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["CHEST PAIN^Chest pain", "SURGERY^Surgical procedure"]
    },
    "PV2.4": {
      "name": "Transfer Reason",
      "description": "Reason for patient transfer",
      "dataType": "CE",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["ACUITY^Increased acuity", "BED^Bed availability"]
    },
    "PV2.5": {
      "name": "Patient Valuables",
      "description": "Description of patient valuables",
      "dataType": "ST",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "examples": ["Wallet, watch, ring", "None"]
    },
    "PV2.6": {
      "name": "Patient Valuables Location",
      "description": "Location where patient valuables are stored",
      "dataType": "ST",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["Safe deposit", "With family", "Room 201 closet"]
    },
    "PV2.7": {
      "name": "Visit User Code",
      "description": "User-defined visit classification",
      "dataType": "IS",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "table": "0130"
    },
    "PV2.8": {
      "name": "Expected Admit Date",
      "description": "Expected admission date",
      "dataType": "TS",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["20220816090000", "20221201"]
    },
    "PV2.9": {
      "name": "Expected Discharge Date",
      "description": "Expected discharge date",
      "dataType": "TS",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["20220820", "20221205"]
    },
    "PV2.10": {
      "name": "Estimated Length of Inpatient Stay",
      "description": "Estimated number of days for inpatient stay",
      "dataType": "NM",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["3", "7", "14"]
    },
    "PV2.11": {
      "name": "Actual Length of Inpatient Stay",
      "description": "Actual number of days for inpatient stay",
      "dataType": "NM",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["4", "8", "12"]
    },
    "PV2.12": {
      "name": "Visit Description",
      "description": "Descriptive text about the visit",
      "dataType": "ST",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["Routine follow-up", "Emergency admission", "Scheduled surgery"]
    },
    "PV2.13": {
      "name": "Referral Source Code",
      "description": "Source of referral for this visit",
      "dataType": "XCN",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "examples": ["12345^JONES^MARY^MD", "67890^SMITH^JOHN^NP"]
    },
    "PV2.14": {
      "name": "Previous Service Date",
      "description": "Date of previous service",
      "dataType": "DT",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["20220701", "20221101"]
