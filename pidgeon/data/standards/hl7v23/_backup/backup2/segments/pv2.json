    "PV2.21": {
      "name": "Visit Publicity Code",
      "description": "Publicity restrictions for visit",
      "dataType": "IS",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "table": "0215"
    },
    "PV2.22": {
      "name": "Visit Protection Indicator",
      "description": "Protection indicator for visit information",
      "dataType": "ID",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "table": "0136",
      "examples": ["Y", "N"]
    },
    "PV2.23": {
      "name": "Clinic Organization Name",
      "description": "Name of clinic organization",
      "dataType": "XON",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "examples": ["Main Campus Clinic", "Outpatient Surgery Center"]
