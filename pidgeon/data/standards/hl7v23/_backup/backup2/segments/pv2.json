    "PV2.24": {
      "name": "Patient Status Code",
      "description": "Current status of patient",
      "dataType": "IS",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "table": "0216"
    },
    "PV2.25": {
      "name": "Visit Priority Code",
      "description": "Priority level for visit",
      "dataType": "IS",
      "usage": "O",
