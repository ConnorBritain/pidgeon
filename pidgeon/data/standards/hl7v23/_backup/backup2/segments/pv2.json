    "PV2.15": {
      "name": "Employment Illness Related Indicator",
      "description": "Indicates if illness is employment related",
      "dataType": "ID",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "table": "0136",
      "examples": ["Y", "N"]
    },
    "PV2.16": {
      "name": "Purge Status Code",
      "description": "Status of record purging",
      "dataType": "IS",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "table": "0213"
