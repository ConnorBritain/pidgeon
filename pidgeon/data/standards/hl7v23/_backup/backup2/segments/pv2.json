      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "table": "0214"
    },
    "PV2.19": {
      "name": "Retention Indicator",
      "description": "Indicates if record should be retained",
      "dataType": "ID",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "table": "0136",
      "examples": ["Y", "N"]
    },
    "PV2.20": {
      "name": "Expected Number of Insurance Plans",
      "description": "Expected number of insurance plans",
      "dataType": "NM",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["1", "2", "3"]
