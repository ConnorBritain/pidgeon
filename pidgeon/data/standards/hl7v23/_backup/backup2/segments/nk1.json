{
  "segment": "NK1",
  "name": "Next of Kin",
  "description": "Contains patient next of kin, emergency contacts, and associated parties information",
  "standard": "hl7v23",
  "version": "2.3",
  "usage": "O",
  "cardinality": {
    "min": 0,
    "max": "unbounded"
  },
  "fields": {
    "NK1.1": {
      "name": "Set ID - Next of Kin",
      "description": "Sequence number for NK1 segments within message",
      "dataType": "SI",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1},
      "examples": ["1", "2", "3"]
    },
    "NK1.2": {
      "name": "NK Name",
      "description": "Name of the next of kin or associated party",
      "dataType": "XPN",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["SMITH^JOHN^Q", "DOE^JANE^MARIE"]
    },
    "NK1.3": {
      "name": "Relationship",
      "description": "Relationship of next of kin to patient",
      "dataType": "CE",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "table": "0063",
      "examples": ["SPO^Spouse^HL70063", "CHD^Child^HL70063"]
    },
    "NK1.4": {
      "name": "Address",
      "description": "Address of next of kin",
      "dataType": "XAD",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "examples": ["123 MAIN ST^^CLEVELAND^OH^44101"]
    },
    "NK1.5": {
      "name": "Phone Number",
      "description": "Primary phone number for next of kin",
      "dataType": "XTN",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "examples": ["(216) 555-1234^PRN^PH", "2165551234^WPN^PH"]
    },
    "NK1.6": {
      "name": "Business Phone Number",
      "description": "Business phone number for next of kin",
      "dataType": "XTN",
