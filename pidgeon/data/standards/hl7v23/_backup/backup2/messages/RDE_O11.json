{
  "messageType": "RDE",
  "triggerEvent": "O11",
  "name": "Pharmacy/Treatment Encoded Order",
  "description": "Sent to transmit encoded pharmacy orders with detailed medication instructions",
  "standard": "hl7v23",
  "version": "2.3",
  "category": "Pharmacy Orders",
  "structure": [
    {
      "type": "segment",
      "segment": "MSH",
      "name": "Message Header",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1},
      "notes": ["MSH.9 must be 'RDE^O11'"]
    },
    {
      "type": "segment",
      "segment": "NTE",
      "name": "Notes and Comments",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "notes": ["Message-level notes"]
    },
    {
      "type": "segment",
      "segment": "PID",
      "name": "Patient Identification",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "notes": ["Patient information for medication order"]
    },
    {
      "type": "segment",
      "segment": "PD1",
      "name": "Additional Demographics",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1}
    },
    {
      "type": "segment",
      "segment": "NTE",
      "name": "Notes and Comments",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "notes": ["Patient-level notes"]
    },
    {
      "type": "segment",
      "segment": "PV1",
      "name": "Patient Visit",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "notes": ["Visit context for medication order"]
    },
    {
      "type": "segment",
      "segment": "PV2",
      "name": "Patient Visit - Additional Information",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1}
    },
    {
      "type": "segment",
      "segment": "IN1",
      "name": "Insurance",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "notes": ["Insurance information for pharmacy billing"]
    },
    {
      "type": "segment",
      "segment": "IN2",
      "name": "Insurance Additional Information",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"}
    },
    {
      "type": "segment",
      "segment": "IN3",
      "name": "Insurance Additional Information - Certification",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"}
    },
    {
      "type": "segment",
      "segment": "GT1",
      "name": "Guarantor",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1}
    },
    {
      "type": "segment",
      "segment": "AL1",
      "name": "Patient Allergy Information",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "notes": ["Critical for medication safety"]
    },
    {
      "type": "segment",
      "segment": "ORC",
      "name": "Common Order",
      "usage": "R",
      "cardinality": {"min": 1, "max": "unbounded"},
      "notes": ["Order control information", "Can repeat for multiple medication orders"]
    },
    {
      "type": "segment",
      "segment": "RXE",
      "name": "Pharmacy/Treatment Encoded Order",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1},
      "notes": ["Detailed medication order with dosing instructions"]
    },
    {
      "type": "segment",
      "segment": "NTE",
      "name": "Notes and Comments",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "notes": ["Medication order notes"]
    },
    {
      "type": "segment",
      "segment": "RXR",
      "name": "Pharmacy/Treatment Route",
      "usage": "R",
      "cardinality": {"min": 1, "max": "unbounded"},
      "notes": ["Route of administration", "Can repeat for multiple routes"]
    },
    {
      "type": "segment",
      "segment": "RXC",
      "name": "Pharmacy/Treatment Component Order",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "notes": ["Components of compound medications"]
    },
    {
      "type": "segment",
      "segment": "OBX",
      "name": "Observation/Result",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "notes": ["Observations supporting the medication order"]
    },
    {
      "type": "segment",
      "segment": "NTE",
      "name": "Notes and Comments",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"},
      "notes": ["Observation-level notes"]
    },
    {
      "type": "segment",
      "segment": "CTI",
      "name": "Clinical Trial Identification",
      "usage": "O",
      "cardinality": {"min": 0, "max": "unbounded"}
    }
  ],
  "useCases": [
    "New prescription orders",
    "Medication order modifications",
    "Complex medication regimens",
    "Compound medication orders"
  ],
  "examples": [
    "MSH|^~\\&|PHARMACY|HOSPITAL|||20231201120000||RDE^O11|123456796|P|2.3",
    "PID|||12345678^^^HOSPITAL^MR||DOE^JOHN^MIDDLE||19800101|M",
    "PV1||I|3E^301^A^HOSPITAL^R",
    "ORC|NW|RX001|||||||20231201120000|||DOC123^SMITH^JANE",
    "RXE|1^Q6H^Q6H^^20231201120000^20231210120000|ASPIRIN^Aspirin 325mg TAB^NDC|325||TAB||||||||DOC123^SMITH^JANE",
    "RXR|PO^ORAL^HL70162"
  ]
}