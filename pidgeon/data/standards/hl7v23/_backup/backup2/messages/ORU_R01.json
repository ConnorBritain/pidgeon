{
  "messageType": "ORU",
  "triggerEvent": "R01",
  "messageStructure": "ORU_R01",
  "name": "Unsolicited Transmission of Observation Message",
  "description": "Sent to transmit laboratory results, radiology reports, or other observation data",
  "standard": "hl7v23",
  "version": "2.3",
  "category": "Observation Reporting",
  "chapter": "7",
  "chapterName": "Observation Reporting",
  "businessPurpose": "Transmit observation results, laboratory values, radiology reports, and other clinical findings",
  "timing": "Sent when results are available, verified, and ready for clinical review",
