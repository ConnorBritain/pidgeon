{
  "messageType": "ADT",
  "triggerEvent": "A13",
  "name": "Cancel Discharge/End Visit",
  "description": "Sent to cancel a previously sent discharge or end visit notification message",
  "standard": "hl7v23",
  "version": "2.3",
  "category": "Patient Administration",
  "structure": [
    {
      "type": "segment",
      "segment": "MSH",
      "name": "Message Header",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1},
      "notes": ["MSH.9 must be 'ADT^A13'"]
    },
    {
      "type": "segment",
      "segment": "EVN",
      "name": "Event Type",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1}
    },
    {
      "type": "segment",
      "segment": "PID",
      "name": "Patient Identification",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1}
    },
    {
      "type": "segment",
      "segment": "PD1",
      "name": "Additional Demographics",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1}
    },
    {
      "type": "segment",
      "segment": "PV1",
      "name": "Patient Visit",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1}
    },
    {
      "type": "segment",
      "segment": "PV2",
      "name": "Patient Visit - Additional Information",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1}
    },
    {
      "type": "segment",
      "segment": "DB1",
      "name": "Disability Information",
      "usage": "O",
      "cardinality": {"min": 0, "max": 0}
    },
    {
      "type": "segment",
      "segment": "OBX",
      "name": "Observation/Result",
      "usage": "O",
      "cardinality": {"min": 0, "max": 0}
    }
  ],
  "useCases": [
    "Cancel premature discharge notification",
    "Reverse discharge due to medical complication",
    "Cancel erroneous end-of-visit message",
    "Administrative correction for discharge errors"
  ],
  "examples": [
    "MSH|^~\\&|HIS|HOSPITAL|||20231201130000||ADT^A13|MSG000013|P|2.3",
    "EVN|A13|20231201130000|||DIS123^WILSON^ROBERT",
    "PID|1||12345678^^^HOSPITAL^MR||DOE^JOHN^MIDDLE||19800101|M|||123 MAIN ST^^CITY^ST^12345||(555)123-4567",
    "PV1|1|I|3E^301^A^HOSPITAL^R|||DOC123^SMITH^JANE|MED|||19||||||DOC123^SMITH^JANE|INS|12345678|A"
  ]
}