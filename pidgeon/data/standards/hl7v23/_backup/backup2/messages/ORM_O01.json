{
  "messageType": "ORM",
  "triggerEvent": "O01",
  "name": "Order Message",
  "description": "Sent to transmit new orders (laboratory, radiology, pharmacy, etc.)",
  "standard": "hl7v23",
  "version": "2.3",
  "category": "Order Entry",
  "structure": [
    {
      "type": "segment",
      "segment": "MSH",
      "name": "Message Header",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1},
      "notes": ["MSH.9 must be 'ORM^O01'"]
    },
    {
      "type": "segment",
      "segment": "NTE",
      "name": "Notes and Comments",
      "usage": "O",
      "cardinality": {"min": 0, "max": 0}
    },
    {
      "type": "group",
      "name": "PATIENT",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "structure": [
        {
          "type": "segment",
          "segment": "PID",
          "name": "Patient Identification",
          "usage": "R",
          "cardinality": {"min": 1, "max": 1}
        },
        {
          "type": "segment",
          "segment": "PD1",
          "name": "Additional Demographics",
          "usage": "O",
          "cardinality": {"min": 0, "max": 1}
        },
        {
          "type": "segment",
          "segment": "NTE",
          "name": "Notes and Comments",
          "usage": "O",
          "cardinality": {"min": 0, "max": 0}
        },
        {
          "type": "group",
          "name": "PATIENT_VISIT",
          "usage": "O",
          "cardinality": {"min": 0, "max": 1},
          "structure": [
            {
              "type": "segment",
              "segment": "PV1",
              "name": "Patient Visit",
              "usage": "R",
              "cardinality": {"min": 1, "max": 1}
            },
            {
              "type": "segment",
              "segment": "PV2",
              "name": "Patient Visit - Additional Information",
              "usage": "O",
              "cardinality": {"min": 0, "max": 1}
            }
          ]
        },
        {
          "type": "group",
          "name": "INSURANCE",
          "usage": "O",
          "cardinality": {"min": 0, "max": 0},
          "structure": [
            {
              "type": "segment",
              "segment": "IN1",
              "name": "Insurance",
              "usage": "R",
              "cardinality": {"min": 1, "max": 1}
            },
            {
              "type": "segment",
              "segment": "IN2",
              "name": "Insurance Additional Information",
              "usage": "O",
              "cardinality": {"min": 0, "max": 1}
            },
            {
              "type": "segment",
              "segment": "IN3",
              "name": "Insurance Additional Information - Certification",
              "usage": "O",
              "cardinality": {"min": 0, "max": 1}
            }
          ]
        },
        {
          "type": "segment",
          "segment": "GT1",
          "name": "Guarantor",
          "usage": "O",
          "cardinality": {"min": 0, "max": 1}
        },
        {
          "type": "segment",
          "segment": "AL1",
          "name": "Patient Allergy Information",
          "usage": "O",
          "cardinality": {"min": 0, "max": 0}
        }
      ]
    },
    {
      "type": "group",
      "name": "ORDER",
      "usage": "R",
      "cardinality": {"min": 1, "max": "unbounded"},
      "structure": [
        {
          "type": "segment",
          "segment": "ORC",
          "name": "Common Order",
          "usage": "R",
          "cardinality": {"min": 1, "max": 1}
        },
        {
          "type": "group",
          "name": "ORDER_DETAIL",
          "usage": "O",
          "cardinality": {"min": 0, "max": 1},
          "structure": [
            {
              "type": "choice",
              "name": "ORDER_TYPE",
              "usage": "O",
              "cardinality": {"min": 0, "max": 0},
              "structure": [
                {
                  "type": "segment",
                  "segment": "OBR",
                  "name": "Observation Request",
                  "usage": "R",
                  "cardinality": {"min": 1, "max": 1}
                },
                {
                  "type": "segment",
                  "segment": "RQD",
                  "name": "Requisition Detail",
                  "usage": "R",
                  "cardinality": {"min": 1, "max": 1}
                },
                {
                  "type": "segment",
                  "segment": "RQ1",
                  "name": "Requisition Detail-1",
                  "usage": "R",
                  "cardinality": {"min": 1, "max": 1}
                },
                {
                  "type": "segment",
                  "segment": "RXO",
                  "name": "Pharmacy/Treatment Order",
                  "usage": "R",
                  "cardinality": {"min": 1, "max": 1}
                },
                {
                  "type": "segment",
                  "segment": "ODS",
                  "name": "Dietary Orders, Supplements, and Preferences",
                  "usage": "R",
                  "cardinality": {"min": 1, "max": 1}
                },
                {
                  "type": "segment",
                  "segment": "ODT",
                  "name": "Diet Tray Instructions",
                  "usage": "R",
                  "cardinality": {"min": 1, "max": 1}
                }
              ]
            },
            {
              "type": "segment",
              "segment": "NTE",
              "name": "Notes and Comments",
              "usage": "O",
              "cardinality": {"min": 0, "max": 0}
            },
            {
              "type": "segment",
              "segment": "DG1",
              "name": "Diagnosis",
              "usage": "O",
              "cardinality": {"min": 0, "max": 0}
            },
            {
              "type": "group",
              "name": "OBSERVATION",
              "usage": "O",
              "cardinality": {"min": 0, "max": 0},
              "structure": [
                {
                  "type": "segment",
                  "segment": "OBX",
                  "name": "Observation/Result",
                  "usage": "R",
                  "cardinality": {"min": 1, "max": 1}
                },
                {
                  "type": "segment",
                  "segment": "NTE",
                  "name": "Notes and Comments",
                  "usage": "O",
                  "cardinality": {"min": 0, "max": 0}
                }
              ]
            }
          ]
        },
        {
          "type": "segment",
          "segment": "CTI",
          "name": "Clinical Trial Identification",
          "usage": "O",
          "cardinality": {"min": 0, "max": 1}
        },
        {
          "type": "segment",
          "segment": "BLG",
          "name": "Billing",
          "usage": "O",
          "cardinality": {"min": 0, "max": 1}
        }
      ]
    }
  ],
  "useCases": [
    "Laboratory test orders",
    "Radiology procedure orders",
    "Pharmacy medication orders",
    "Dietary orders"
  ],
  "examples": [
    "MSH|^~\\&|CPOE|HOSPITAL|||20231201110000||ORM^O01|123456795|P|2.3",
    "PID|||12345678^^^HOSPITAL^MR||DOE^JOHN^MIDDLE||19800101|M",
    "PV1||I|3E^301^A^HOSPITAL^R",
    "ORC|NW|ORDER001|||||||20231201110000|||DOC123^SMITH^JANE",
    "OBR|1||LAB001^CBC with Differential^LAB|||20231201110000"
  ]
}