{
  "messageType": "ORR",
  "triggerEvent": "O02",
  "name": "Order Response",
  "description": "Sent to acknowledge receipt and provide status of order messages",
  "standard": "hl7v23",
  "version": "2.3",
  "category": "Order Management",
  "structure": [
    {
      "type": "segment",
      "segment": "MSH",
      "name": "Message Header",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1},
      "notes": ["MSH.9 must be 'ORR^O02'"]
    },
    {
      "type": "segment",
      "segment": "MSA",
      "name": "Message Acknowledgment",
      "usage": "R",
      "cardinality": {"min": 1, "max": 1}
    },
    {
      "type": "segment",
      "segment": "ERR",
      "name": "Error",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1}
    },
    {
      "type": "segment",
      "segment": "NTE",
      "name": "Notes and Comments",
      "usage": "O",
      "cardinality": {"min": 0, "max": 0}
    },
    {
      "type": "group",
      "name": "RESPONSE",
      "usage": "O",
      "cardinality": {"min": 0, "max": 1},
      "structure": [
        {
          "type": "group",
          "name": "PATIENT",
          "usage": "O",
          "cardinality": {"min": 0, "max": 1},
          "structure": [
            {
              "type": "segment",
              "segment": "PID",
              "name": "Patient Identification",
              "usage": "R",
              "cardinality": {"min": 1, "max": 1}
            },
            {
              "type": "segment",
              "segment": "NTE",
              "name": "Notes and Comments",
              "usage": "O",
              "cardinality": {"min": 0, "max": 0}
            }
          ]
        },
        {
          "type": "group",
          "name": "ORDER",
          "usage": "R",
          "cardinality": {"min": 1, "max": "unbounded"},
          "structure": [
            {
              "type": "segment",
              "segment": "ORC",
              "name": "Common Order",
              "usage": "R",
              "cardinality": {"min": 1, "max": 1}
            },
            {
              "type": "group",
              "name": "ORDER_DETAIL",
              "usage": "O",
              "cardinality": {"min": 0, "max": 1},
              "structure": [
                {
                  "type": "segment",
                  "segment": "OBR",
                  "name": "Observation Request",
                  "usage": "R",
                  "cardinality": {"min": 1, "max": 1}
                },
                {
                  "type": "segment",
                  "segment": "NTE",
                  "name": "Notes and Comments",
                  "usage": "O",
                  "cardinality": {"min": 0, "max": 0}
                },
                {
                  "type": "segment",
                  "segment": "CTI",
                  "name": "Clinical Trial Identification",
                  "usage": "O",
                  "cardinality": {"min": 0, "max": 0}
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "useCases": [
    "Laboratory order acknowledgments",
    "Radiology order confirmations",
    "Procedure request status updates",
    "Order rejection notifications"
  ],
  "examples": [
    "MSH|^~\\&|LAB|HOSPITAL|||20231201140000||ORR^O02|MSG000014|P|2.3",
    "MSA|AA|MSG000001|Order received and accepted|",
    "PID|||12345678^^^HOSPITAL^MR||DOE^JOHN^MIDDLE||19800101|M",
    "ORC|OK|LAB001|||||||20231201140000|||DOC123^SMITH^JANE",
    "OBR|1|LAB001||CBC^Complete Blood Count^LN|||20231201140000"
  ]
}